{

	"manifest_version": 2,
	"name": "Go To Dell",
	"version": "0.1",

	"homepage_url": "http://github.com/ajpeake/goToDell.git",

	"description": "Adds a browser action and goes to dell.com when the user clicks on the icon.",

	"icons": {
		"64": "dell-64.png"
	},

	"applications": {
		"gecko": {
			"id": "ajpeake@gmail.com",
			"strict_min_version": "45.0"
		}
	},

	"background": {
		"scripts": ["background.js"]
	},

	"browser_action": {
		"default_title": "Go To Dell.com",
		"default_icon": "dell-64.png"
	}

}
